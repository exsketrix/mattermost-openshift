apiVersion: image.openshift.io/v1
kind: ImageStream
metadata:
  labels:
    app: mattermostdb
  name: mattermostdb
spec:
  lookupPolicy:
    local: false
  tags:
    - from:
        kind: DockerImage
        name: '{{mattermost_openshift_docker_registry_host}}:{{mattermost_openshift_docker_registry_port}}/rhscl/postgresql-94-rhel7:latest'
      name: {{mattermost_openshift_internal_tag}}
      importPolicy: {}
      referencePolicy:
        type: Source